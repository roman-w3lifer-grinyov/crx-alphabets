{
  "manifest_version": 2,
  "name": "Alphabet",
  "description": "Uppercase and lowercase alphabets",
  "version": "1.2.4",
  "icons": {
    "16": "img/icons/letter-a/16x16.png",
    "48": "img/icons/letter-a/48x48.png",
    "128": "img/icons/letter-a/128x128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icons/letter-a/16x16.png",
      "48": "img/icons/letter-a/48x48.png",
      "128": "img/icons/letter-a/128x128.png"
    },
    "default_title": "Alphabet",
    "default_popup": "html/popup.html"
  },
  "permissions": [
    "storage"
  ]
}
